package com.example.threadmessage;

public class NewThread extends Thread {

	public NewThread() {
		// TODO Auto-generated constructor stub
	}

	public NewThread(Runnable runnable) {
		super(runnable);
		// TODO Auto-generated constructor stub
	}

	public NewThread(String threadName) {
		super(threadName);
		// TODO Auto-generated constructor stub
	}

	public NewThread(Runnable runnable, String threadName) {
		super(runnable, threadName);
		// TODO Auto-generated constructor stub
	}

	public NewThread(ThreadGroup group, Runnable runnable) {
		super(group, runnable);
		// TODO Auto-generated constructor stub
	}

	public NewThread(ThreadGroup group, String threadName) {
		super(group, threadName);
		// TODO Auto-generated constructor stub
	}

	public NewThread(ThreadGroup group, Runnable runnable, String threadName) {
		super(group, runnable, threadName);
		// TODO Auto-generated constructor stub
	}

	public NewThread(ThreadGroup group, Runnable runnable, String threadName,
			long stackSize) {
		super(group, runnable, threadName, stackSize);
		// TODO Auto-generated constructor stub
	}

}
